{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/vercel.js"
    }
  ],
  "functions": {
    "api/vercel.js": {
      "includeFiles": "**/*",
      "excludeFiles": [
        "**/*.test.js",
        "**/node_modules/mongodb-memory-server/**",
        "**/node_modules/@jest/**",
        "**/node_modules/jest/**",
        "**/node_modules/eslint/**",
        "**/node_modules/prettier/**",
        "**/node_modules/nodemon/**",
        "**/node_modules/supertest/**",
        "**/node_modules/@babel/**",
        "coverage/**",
        "tests/**"
      ]
    }
  }
}
